amount of trades: <%= trades.length %>
  <table class="table table-hover">
    <thead class="thead-dark">
    <tr>
      <th scope="col">ID</th>
      <th scope="col">type</th>
      <th scope="col">price</th>
      <th scope="col">cost</th>
      <th scope="col">fee</th>
      <th scope="col">volume</th>
    </tr>
    </thead>
    <tbody>
<% trades.each do |tid , data| %>
      <tr>
      <th scope="row"><a href="/trade/<%= tid %>"><%= tid %></a></th>
      <td><%= data['type'] %></td>
      <td><%= data['price'] %></td>
      <td><%= data['cost'] %></td>
      <td><%= data['fee'] %></td>
      <td><%= data['vol'] %></td>
      </tr>
<% end %>
    </tbody>
  </table>
